   "source": [
    "# 建構費氏數列\n",
    "fib = [1,1]\n",
    "ratio = [1]\n",
    "n = int(input('請輸入所需費氏數列項數：'))\n",
    "for i in range(2,n):\n",
    "    fib_new = fib[i-1]+fib[i-2]\n",
    "    fib.append(fib_new)\n",
    "    ratio_new = fib[i-1] / fib_new\n",
    "    ratio.append(ratio_new)\n",
    "print(fib)\n",
    "print(ratio)"
   ]
